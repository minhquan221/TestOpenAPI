<div class="areabatch">
    <h4 class="header-title">Batch @ViewBag.CountNew</h4>
    <p class="m-b-30"></p>
    <div class="row">
        <div class="col-md-12">
            <div class="form-group row">
                <label class="col-md-2 col-form-label">payeeAccountNumber</label>
                <div class="col-md-6">
                    <input class="form-control" type="text" placeholder="payeeAccountNumber" name="payeeAccountNumber" value="" />
                </div>
                <div class="col-md-2"></div>
                <div class="col-md-2"></div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="form-group row">
                <label class="col-md-2 col-form-label">payeeName</label>
                <div class="col-md-6">
                    <input class="form-control" type="text" placeholder="payeeName" name="payeeName" value="" />
                </div>
                <div class="col-md-2"></div>
                <div class="col-md-2"></div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="form-group row">
                <label class="col-md-2 col-form-label">amount</label>
                <div class="col-md-6">
                    <input class="form-control" type="text" placeholder="amount" name="amount" value="" />
                </div>
                <div class="col-md-2"></div>
                <div class="col-md-2"></div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="form-group row">
                <label class="col-md-2 col-form-label">paymentDescription</label>
                <div class="col-md-6">
                    <input class="form-control" type="text" placeholder="paymentDescription" name="paymentDescription" value="" />
                </div>
                <div class="col-md-2"></div>
                <div class="col-md-2"></div>
            </div>
        </div>
    </div>
</div>